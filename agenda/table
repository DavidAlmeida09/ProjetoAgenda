CREATE TABLE `tableEqui` (
	`id` INT(11) NOT NULL AUTO_INCREMENT,
	`equipamento` VARCHAR(220) NOT NULL COLLATE 'latin1_swedish_ci',
	`descricao` VARCHAR(500) NOT NULL COLLATE 'latin1_swedish_ci',
	`status` VARCHAR(500) NOT NULL COLLATE 'latin1_swedish_ci',
	`empresa` VARCHAR(500) NOT NULL COLLATE 'latin1_swedish_ci',
	`valor` FLOAT NULL DEFAULT NULL,
	`created` DATETIME NOT NULL,
	`modifed` DATETIME NULL DEFAULT NULL,
	PRIMARY KEY (`id`) USING BTREE
)
COLLATE='latin1_swedish_ci'
ENGINE=InnoDB
AUTO_INCREMENT=7
;
